
generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
  moduleFormat    =  "cjs"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id  String  @id @default(uuid())
  username String @unique
  password String
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @map("updated_at")

  @@map("users")
}
